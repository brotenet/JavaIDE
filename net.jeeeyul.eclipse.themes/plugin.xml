         <property-name
               name="jtab-item-horizontal-spacing">
         </property-name>
         <property-name
               name="jtab-chevron-color">
         </property-name>
         <property-name
               name="jtab-header-background">
         </property-name>
         <property-name
               name="jtab-selected-tab-background">
         </property-name>
         <property-name
               name="jtab-use-ellipses">
         </property-name>
         <property-name
               name="jtab-truncate-tab-items">
         </property-name>
         <property-name
               name="jtab-minimum-characters">
         </property-name>
         <property-name
               name="jtab-close-button-alignment">
         </property-name>
      </handler>
      <handler
            adapter="org.eclipse.e4.ui.css.swt.dom.CompositeElement"
            handler="net.jeeeyul.eclipse.themes.css.internal.handlers.JStyledTextPropertyHandler">
         <property-name
               name="jeditor-line-color">
         </property-name>
         <property-name
               name="jeditor-line-style">
         </property-name>
         <property-name
               name="jeditor-range-indicator-color">
         </property-name>
      </handler>
      <handler
            adapter="org.eclipse.e4.ui.css.swt.dom.ShellElement"
            handler="net.jeeeyul.eclipse.themes.css.internal.handlers.ShellCSSHandler">
         <property-name
               name="jswt-alpha">
         </property-name>
      </handler>
      <handler
            adapter="org.eclipse.e4.ui.css.swt.dom.ToolBarElement"
            handler="net.jeeeyul.eclipse.themes.css.internal.handlers.ToolBarCSSPropertyHandler">
         <property-name
               name="jtool-item-color">
         </property-name>
      </handler>
      <handler
            adapter="net.jeeeyul.eclipse.themes.css.internal.elements.FormHeadingElement"
            handler="net.jeeeyul.eclipse.themes.css.internal.handlers.FormHeadingCSSPropertyHandler">
         <property-name
               name="jtext-background">
         </property-name>
         <property-name
               name="jbottom-keyline-1-color">
         </property-name>
         <property-name
               name="jbottom-keyline-2-color">
         </property-name>
