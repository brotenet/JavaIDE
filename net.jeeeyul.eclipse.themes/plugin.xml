      <handler
            adapter="org.eclipse.e4.ui.css.swt.dom.CompositeElement"
            handler="net.jeeeyul.eclipse.themes.css.internal.handlers.SashWidthHandler">
         <property-name
               name="jsash-width">
         </property-name>
      </handler>
      <handler
            adapter="org.eclipse.e4.ui.css.swt.dom.CTabFolderElement"
            handler="net.jeeeyul.eclipse.themes.css.internal.handlers.JTabCSSPropertyHandler">
         <property-name
               name="jtab-border-color">
         </property-name>
         <property-name
               name="jtab-selected-border-color">
         </property-name>
         <property-name
               name="jtab-unselected-border-color">
         </property-name>
         <property-name
               name="jtab-border-radius">
         </property-name>
         <property-name
               name="jtab-spacing">
         </property-name>
         <property-name
               name="jtab-close-button-color">
         </property-name>
         <property-name
               name="jtab-close-button-hot-color">
         </property-name>
         <property-name
               name="jtab-close-button-active-color">
         </property-name>
         <property-name
               name="jtab-close-button-line-width">
         </property-name>
         <property-name
               name="jtab-unselected-tabs-background">
         </property-name>
         <property-name
               name="jtab-hover-tabs-background">
         </property-name>
         <property-name
               name="jtab-hover-border-color">
         </property-name>
         <property-name
               name="jtab-hover-color">
         </property-name>
         <property-name
               name="jtab-margin">
         </property-name>
         <property-name
               name="jtab-padding">
         </property-name>
         <property-name
               name="jtab-shadow-color">
         </property-name>
         <property-name
               name="jtab-shadow-radius">
         </property-name>
         <property-name
               name="jtab-shadow-position">
         </property-name>
         <property-name
               name="jtab-selected-text-shadow-color">
         </property-name>
         <property-name
               name="jtab-unselected-text-shadow-color">
         </property-name>
         <property-name
               name="jtab-hover-text-shadow-color">
         </property-name>
         <property-name
