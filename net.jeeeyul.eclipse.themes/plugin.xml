<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.e4.ui.css.core.elementProvider">
      <provider
            class="net.jeeeyul.eclipse.themes.css.internal.elements.ExtendedElementProvider">
         <widget
               class="org.eclipse.ui.internal.forms.widgets.FormHeading"></widget>
         <widget
               class="org.eclipse.ui.forms.widgets.FormText">
         </widget>
      </provider>
   </extension>
