      <handler
            adapter="org.eclipse.ui.internal.forms.css.dom.SectionElement"
            handler="net.jeeeyul.eclipse.themes.css.internal.handlers.SectionCSSPropertyHandler">
         <property-name
               name="jtitle-bar-border-color">
         </property-name>
         <property-name
               name="jtitle-bar-background-color">
         </property-name>
         <property-name
               name="jtitle-bar-text-color">
         </property-name>
         <property-name
               name="jtitle-bar-active-text-color">
         </property-name>
      </handler>
      <handler
            adapter="net.jeeeyul.eclipse.themes.css.internal.elements.FormTextElement"
            handler="net.jeeeyul.eclipse.themes.css.internal.handlers.FormTextCSSPropertyHandler">
         <property-name
               name="hyperlink-color">
         </property-name>
         <property-name
               name="active-hyperlink-color">
         </property-name>
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="net.jeeeyul.eclipse.themes.internal.StartUp">
      </startup>
   </extension>
   <extension
         id="id1"
         point="org.eclipse.e4.workbench.model">
      <processor
            apply="always"
            beforefragment="true"
            class="net.jeeeyul.eclipse.themes.DynamicResourceProcessor">
      </processor>
      <processor
            apply="always"
            beforefragment="true"
            class="net.jeeeyul.eclipse.themes.EmptyPartStackProcessor">
      </processor>
   </extension>
</plugin>
