		<editor id="staticField">
			<parameter name="class">javax.swing.SwingConstants</parameter>
			<parameter name="fields">LEFT CENTER RIGHT LEADING TRAILING</parameter>
		</editor>
	</property>
	<!-- PROPERTIES: mnemonic -->
